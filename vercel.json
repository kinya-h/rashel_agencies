{
  "version": 2,
  "builds": [
    {
      "src": "rashel_agencies/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    { "src": "client/build/**", "use": "@vercel/static" },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "client/build/**",
      "dest": "/clientt/build/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "rashel_agencies/wsgi.py"
    }
  ]
}
